      "parameters": {
        "url": "=https://gmail.googleapis.com/gmail/v1/users/me/messages/{{ $json.messageId }}",
        "authentication": "predefinedCredentialType",
        "nodeCredentialType": "gmailOAuth2Api",
        "options": {}
      },
      "id": "get-message-details",
      "name": "Get Message Details",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [
        1120,
        300
      ],
      "credentials": {
        "gmailOAuth2Api": {
          "id": "",
          "name": "Gmail account"
        }
      }
    },
    {
      "parameters": {
        "jsCode": "// Filter messages by year\nconst message = $input.first().json;\nconst config = $('Set Configuration').first().json;\nconst targetYear = parseInt(config.deleteBeforeYear);\n\n// Extract date\nlet messageDate = null;\nif (message.internalDate) {\n  messageDate = new Date(parseInt(message.internalDate));\n} else if (message.payload && message.payload.headers) {\n  const dateHeader = message.payload.headers.find(h => h.name.toLowerCase() === 'date');\n  if (dateHeader) {\n    messageDate = new Date(dateHeader.value);\n  }\n}\n\nif (messageDate && messageDate.getFullYear() < targetYear) {\n  return [{\n    json: {\n      messageId: message.id,\n      threadId: message.threadId,\n      subject: message.payload?.headers?.find(h => h.name === 'Subject')?.value || 'No Subject',\n      from: message.payload?.headers?.find(h => h.name === 'From')?.value || 'Unknown',\n      date: messageDate.toISOString().split('T')[0],\n      year: messageDate.getFullYear(),\n      shouldDelete: true\n    }\n  }];\n}\n\nreturn [];"
      },
      "id": "filter-messages",
      "name": "Filter Messages by Year",
